#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>


                             // 사용자로부터 양의정수를 받아 그 수만큼 3의 배수를 출력하는 프로그램만들기
int main(void)
{

	int n, i = 1;

	printf("양의정수를 입력하세요 : ");
	scanf("%d", &n);
	while (n >= i)
	{
		printf("3의 배수 출력 : %d\n", 3 * i);
		i++;
	}
	system("pause");
	return 0;
}
	   	  
